  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "_moduleAliases": {
    "@middlewares": "dist/middlewares",
    "@routes": "dist/routes",
    "@libs": "dist/libs"
  },
  "scripts": {
    "test": "jest --coverage",
    "build": "tsc",
    "start": "node -r module-alias/register dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only -r tsconfig-paths/register src/index.ts",
    "lint": "eslint 'src/**/*.{ts,tsx}' --fix",
    "format": "prettier --write 'src/**/*.{ts,tsx,js,jsx,json,css,md,mjs}'",
    "prepare": "husky"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx,js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "src/**/*.{json,css,md,mjs}": [
      "prettier --write"
    ]
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/morgan": "^1.9.9",
    "bcryptjs": "^2.4.3",
